<!DOCTYPE html>
<%= render "/refinery/html_tag" %>
  <% site_bar = render('/refinery/site_bar', :head => true) -%>
  <head>
    <%= render "/refinery/head" %>
    <%= render "/refinery/javascripts" %>
  </head>
  <body id="<%= canonical_id @page %>">
    <%= site_bar -%>
    <div id="page_container">

      <header id="header">

        <section id="logo">
          <div class="container">
            <div class="row">
              <div class="col-xs-8 col-xs-offset-2 col-sm-4 col-sm-offset-8 col-md-4 col-md-offset-8">
                <%= image_tag 'HulseDentalLogo2016.png', class: 'img-responsive', alt: 'Hulse Dental Logo' %>
              </div>
            </div>
          </div>
        </section>

        <section id="contact-info">
          <div class="container">
            <div class="row">
              <div class="col-xs-10 col-xs-offset-1 col-sm-4 col-sm-offset-8 col-md-4 col-md-offset-8">
                <p class="text-center">
                  <span>1840 E. Main St, Onalaska</span>
                  <span>608.783.1306</span>
                </p>
              </div>
            </div>
          </div>

        </section>

        <%= render "/refinery/header" -%>
      </header>
      <section id="page">
        <%= yield %>
      </section>
      <footer>
        <%= render "/refinery/footer" -%>
      </footer>
    </div>
  </body>
</html>
